{
  "manifest_version": 2,

  "name": "SR.SE Widget",
  "description": "A widget for playing live channels from Sverige Radio",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
	
  "background": {
    "page": "background.html"
  },

  "content_scripts": [ {
    "js": [ "jquery-2.1.3.min.js", "background.js", "main.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],

  "permissions": []
}